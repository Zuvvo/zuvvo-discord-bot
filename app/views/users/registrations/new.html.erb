 <%- resource_class.omniauth_providers.each do |provider| %>
    <%= form_for "Sign up",
                 url: omniauth_authorize_path(resource_name, provider),
                 method: :post do |form| %>
      <%= form.submit "Sign up", type: "image", src: image_path('discord-login-button.png'), height: 80 %>
    <% end %>
  <% end %>