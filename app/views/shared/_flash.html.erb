<% if flash[:notice] %>
  <div class="alert alert-info mt-4" role ="alert">
    <%= flash[:notice].inspect %>
  </div>
<%end %>

<% if flash[:alert] %>
  <div class="alert alert-warning mt-4" role ="alert">
    <%= flash[:alert] %>
  </div>>
<%end %>

<% if flash[:success] %>
  <div class="alert alert-success mt-4" role ="alert">
    <%= flash[:success] %>
  </div>>
<%end %>

<% if flash[:logged] %>
  <div class="alert alert-success" role ="alert">
    <%= flash[:logged] %>
  </div>>
<%end %>